{
    "compilerOptions": {
        "target": "ES2018",
        "module": "CommonJS",
        "sourceMap": true,
        "lib": [
            "es2020"
        ],
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./*",
            ],
        },
        "typeRoots": [
            "./node_modules/@types"
        ],
        "outDir": "./tsCompile",
        "alwaysStrict": true,
        "noImplicitThis": true,
        "esModuleInterop": true,
        "declaration": false,
    },
    "exclude": [
        "cookieExtractor.js",
        "node_modules",
        "dist",
        "tsCompile",
        "webpack.config.js",
        "tools"
    ]
}